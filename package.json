    "dayjs": "^1.11.12",
    "debounce": "^1.2.1",
    "global": "^4.4.0",
    "history": "^5.3.0",
    "jwt-decode": "^4.0.0",
    "localforage": "^1.10.0",
    "mapbox-gl": "^3.5.2",
    "obstruction": "^2.1.0",
    "prop-types": "^15.8.1",
