{
  "name": "RTZ",
  "version": "0.8.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:development": "env-cmd -f .env.development pnpm run build --mode development",
    "preview": "vite preview --host",
    "lighthouse": "lhci autorun",
    "lint": "eslint src",
    "lint:fix": "pnpm run lint --fix",
    "lint:report": "pnpm run lint --output-file eslint_report.json --format json",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest",
    "test:e2e": "start-server-and-test preview localhost:4173 'cypress run --e2e'",
    "test:e2e:chrome": "start-server-and-test preview localhost:4173 'cypress run --e2e --browser chrome'",
    "test:e2e:firefox": "start-server-and-test preview localhost:4173 'cypress run --e2e --browser firefox'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' localhost:4173 'cypress open --e2e'"
  },
  "type": "module",
  "repository": "https://github.com/USA-RedDragon/rtz-frontend.git",
  "author": "Chris Vickery <chrisinajar@gmail.com>",
  "contributors": [
    "Joost Wooning <jwooning@gmail.com>",
    "Chris Vickery <chrisinajar@gmail.com>",
    "Cameron Clough <cameronjclough@gmail.com>"
  ],
  "private": true,
  "engines": {
    "node": ">=20.16.0"
  },
  "packageManager": "pnpm@9.6.0",
  "dependencies": {
    "@commaai/api": "github:commaai/comma-api#3.1.3",
    "@commaai/my-comma-auth": "^1.4.1",
